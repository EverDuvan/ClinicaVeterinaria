@page
@model ClinicaVet.App.Frontend.Pages.EliminarClienteModel
@using ClinicaVet.App.Dominio
@{
}
<h1>Esta seguro de eliminar este usuario</h1>
<p>Se va ha eliminar el cliente @Model.dueño.nombre</p>
<form method="post" class="mt-3">
    <input hidden asp-for="dueño.cedula"/>
    <button type="submit" class="btn btn-primary">Eliminar</button>
    <a asp-page="./ListaClientes" class="btn btn-primary">Cancelar</a>
</form>
